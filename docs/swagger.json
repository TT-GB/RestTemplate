            "get": {
                "description": "Retrieves scan results filtered by a customer ID",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "scans"
                ],
                "summary": "Get scan details by scan ID",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Scan ID",
                        "name": "scanID",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/main.Scan"
                            }
                        }
                    },
                    "400": {
                        "description": "Missing parameter",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Internal error",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/scansByCustomerID": {
            "get": {
                "description": "Retrieves scan results filtered by a customer ID",
